@article{Nigam2012,
abstract = {Interest in control of multiple autonomous vehicles continues to grow for applications such as weather monitoring, geographical mapping fauna surveys, and extra-terrestrial exploration. The task of persistent surveillance is of particular significance in that the target area needs to be continuously surveyed, minimizing the time between visitations to the same region. This distinction from one-time coverage does not allow a straightforward application of most exploration techniques to the problem, though ideas from these methods can still be used. The aerial vehicle dynamic and endurance constraints add additional complexity to the autonomous control problem, whereas stochastic environments and vehicle failures introduce uncertainty. In this work, we investigate techniques for high-level control, that are scalable, reliable, efficient, and robust to problem dynamics. Next, we suggest a modification to the control policy to account for aircraft dynamic constraints. We also devise a health monitoring policy and a control policy modification to improve performance under endurance constraints. The Vehicle Swarm Technology Laboratory-a hardware testbed developed at Boeing Research and Technology, Seattle, WA, for evaluating a swarm of unmanned air vehicles-is then described, and these control policies are tested in a realistic scenario.},
author = {Nigam, Nikhil and Bieniawski, Stefan and Kroo, Ilan and Vian, John},
file = {:C$\backslash$:/Users/yoavp/OneDrive‏ - Technion/studies/Thesis-NDS/Literature Review/coverage for literature review/06045299.pdf:pdf},
isbn = {1063-6536},
issn = {10636536},
journal = {IEEE Transactions on Control Systems Technology},
keywords = {Control,coordination,exploration,flight test,hardware,multiple UAVs,persistent surveillance,refueling,swarm,unmanned air vehicle (UAV)},
mendeley-groups = {Literature review},
number = {5},
pages = {1236--1251},
title = {{Control of multiple UAVs for persistent surveillance: Algorithm and flight test results}},
volume = {20},
year = {2012}
}
@INPROCEEDINGS{7798244, 
author={S. G. Loizou and C. C. Constantinou}, 
booktitle={2016 IEEE 55th Conference on Decision and Control (CDC)}, 
title={Multi-robot coverage on dendritic topologies under communication constraints}, 
year={2016}, 
pages={43-48}, 
abstract={This paper proposes a novel algorithm for performing multi-robot coverage on networks with dendritic topology where the communication topology is location dependent and where the motion of each robot is constrained by the presence of the other robots in the network. The algorithm provides complete network coverage by the minimum number of robots, maintenance of communication constraints and robot collision avoidance. The minimum number of robots required for coverage is a by-product of the proposed algorithm. The efficiency of the algorithm is demonstrated through simulation studies.}, 
keywords={motion control;multi-robot systems;topology;communication constraints;communication topology;dendritic topologies;multirobot coverage;robot motion;Algorithm design and analysis;Collision avoidance;Network topology;Partitioning algorithms;Robot kinematics;Topology},
month={Dec},}

@INPROCEEDINGS{7798796, 
author={J. M. Palacios-Gasós and E. Montijano and C. Sagüés and S. Llorente}, 
booktitle={2016 IEEE 55th Conference on Decision and Control (CDC)}, 
title={Multi-robot persistent coverage with optimal times}, 
year={2016}, 
pages={3511-3517}, 
abstract={This paper addresses the persistent coverage problem, in which a group of autonomous robots must visit periodically a finite set of interest points and spend some time covering them, which we call coverage time. An optimization problem to calculate the optimal coverage times is formulated, and sufficient conditions for the existence of solution are given. In particular, a linear cost function is considered to solve the problem as a linear program. An iterative algorithm, which runs on the solution, is proposed to reduce the lengths of the predefined paths traveled by the robots in a finite number of iterations while maintaining optimal coverage times. Moreover, path planning is included in the optimization problem, computing specific weights in the cost function, and thus reducing the traveled distances and the total time spent covering. Simulation results demonstrate the performance of the approach.}, 
keywords={iterative methods;linear programming;multi-robot systems;path planning;autonomous robots;iterative algorithm;linear cost function;linear program;multirobot persistent coverage;optimal coverage times;optimization problem;path planning;Buildings;Cleaning;Cost function;Iterative methods;Planning;Robots},
month={Dec},}

